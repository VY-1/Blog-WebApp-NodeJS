<%- include("partials/header"); -%>

<h1>Compose</h1>
<form action="/compose" method="post">

    

    <div class="form-group">
      <label>Title</label>
      <input type="text" class="form-control" name="postTitle" required autofocus>
      <label>Post</label>
      <textarea class="form-control" name="postBody" id="" cols="30" rows="5" required></textarea>
      
    </div>
    <button type="submit" class="btn btn-primary" name="publish">Publish</button>

    
  </form>

<%- include("partials/footer"); -%>
